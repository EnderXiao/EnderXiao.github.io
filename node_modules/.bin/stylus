#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/EnderXiao.github.io/node_modules/.store/stylus@0.63.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/EnderXiao.github.io/node_modules/.store/stylus@0.63.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/stylus@0.63.0/node_modules/stylus/bin/stylus" "$@"
else
  exec node  "$basedir/../.store/stylus@0.63.0/node_modules/stylus/bin/stylus" "$@"
fi
